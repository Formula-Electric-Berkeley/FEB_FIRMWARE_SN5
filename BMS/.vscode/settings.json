{
    "files.associations": {
        "*.h": "c",
        "*.c": "c",
        "*.hpp": "cpp",
        "*.cpp": "cpp",
        "cstdint": "c"
    },
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.loggingLevel": "Information",
    "cmake.cmakePath": "cube-cmake",
    "cmake.configureArgs": [
        "-DCMAKE_COMMAND=cube-cmake"
    ],
    "cmake.debugConfig": {
        "MIMode": "gdb",
        "miDebuggerPath": "${config:STM32VSCodeExtension.cubeCLT.path}/GNU-tools-for-STM32/bin/arm-none-eabi-gdb"
    },
    "cmake.statusbar.advanced": {
        "debug": {
            "visibility": "hidden"
        }
    },
    "cmake.ctest.testExplorerIntegrationEnabled": false,
    "debug.allowBreakpointsEverywhere": true,
    "testing.automaticallyOpenPeekView": "never",
    "testing.defaultGutterClickAction": "run",
    "testMate.cpp.test.executables": "${workspaceFolder}/test/build/bms_tests",
    "testMate.cpp.test.workingDirectory": "${workspaceFolder}/test/build",
    "testMate.cpp.discovery.gracePeriodForMissing": 10000,
    "testMate.cpp.discovery.testListCaching": false,
    "testMate.cpp.test.advancedExecutables": [
        {
            "pattern": "${workspaceFolder}/test/build/bms_tests",
            "name": "BMS Tests",
            "description": "BMS Unit Tests",
            "cwd": "${workspaceFolder}/test/build",
            "env": {}
        }
    ]
}